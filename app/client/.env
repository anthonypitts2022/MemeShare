REACT_APP_NODE_ENV=${NODE_ENV}

#http or https for SSL
REACT_APP_ssl=${ssl}

# MongoDB environments
REACT_APP_MEMESHARER_posts_dbConnection=${MEMESHARER_posts_dbConnection}
REACT_APP_MEMESHARER_user_dbConnection=${MEMESHARER_user_dbConnection}
REACT_APP_MEMESHARER_gateway_dbConnection=${MEMESHARER_gateway_dbConnection}

#Ports
REACT_APP_gatewayms_port=${gatewayms_port}
