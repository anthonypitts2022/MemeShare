# Encryption Auth Token Key file
export REACT_APP_encryptionAuthKeyFilePath=${encryptionAuthKeyFilePath}

# Encryption Auth Token IV file
export REACT_APP_encryptionAuthIVFilePath=${encryptionAuthIVFilePath}

#google API sign in clientID
export REACT_APP_googleSignInClientID=${googleSignInClientID}

#website name
REACT_APP_website_name=${website_name}
